{
	"extends": [
		"eslint:recommended",
		"plugin:@typescript-eslint/recommended"
	],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": [
		"@typescript-eslint"
	],
	"rules": {
		"no-empty": [
			"error",
			{
				"allowEmptyCatch": true
			}
		],
		"@typescript-eslint/no-non-null-assertion": "off"
	}
}
